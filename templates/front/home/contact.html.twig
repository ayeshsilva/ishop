{% extends 'front/layouts/base.html.twig' %}



{% block title %}
    Contact
{% endblock %}


{% block content %}

    <!-- Start Content Page -->
    <div class="container-fluid bg-light py-5">
        <div class="col-md-6 m-auto text-center">
            <h1 class="h1">Contact Us</h1>
            <p>
                Proident, sunt in culpa qui officia deserunt mollit anim id est laborum.
                Lorem ipsum dolor sit amet.
            </p>
        </div>
    </div>

    <!-- Start Map -->
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2624.7998775380483!2d2.3474928174438476!3d48.862026400000005!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e66e19448a942d%3A0x70a96a2e52d5759a!2s77%20Rue%20Rambuteau%2C%2075001%20Paris!5e0!3m2!1sfr!2sfr!4v1655070576640!5m2!1sfr!2sfr"
            width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
    <!-- Ena Map -->

    <!-- Start Contact -->
    <div class="container py-5">
        <div class="row py-5">
            {{ form_start(form) }}
                <div class="row">
                    <div class="form-group col-md-6 mb-3">
                        {{ form_row(form.name) }}
                    </div>
                    <div class="form-group col-md-6 mb-3">
                        {{ form_row(form.email) }}
                    </div>
                </div>
                <div class="mb-3">
                    {{ form_row(form.subject) }}
                </div>
                <div class="mb-3">
                    {{ form_row(form.message) }}
                </div>
                <div class="row">
                    <div class="col text-end mt-2">
                        {{ form_row(form.submit) }}
                    </div>
                </div>
            {{ form_end(form) }}
        </div>
    </div>
    <!-- End Contact -->


{% endblock %}
